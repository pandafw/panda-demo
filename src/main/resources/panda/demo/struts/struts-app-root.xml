<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="struts-app-root" namespace="/" extends="struts-app-default">
		<action name="index" class="panda.demo.action.IndexAction">
			<result name="success" type="freemarker">/index.ftl</result>
		</action>

		<action name="themes" class="panda.demo.action.SuccessAction">
			<result name="success" type="freemarker">/themes.ftl</result>
		</action>

		<action name="languages" class="panda.demo.action.SuccessAction">
			<result name="success" type="freemarker">/languages.ftl</result>
		</action>

		<action name="sampledate_*" class="panda.demo.action.sampledate.SampleDateAction" method="{1}">
			<result name="success" type="freemarker">/panda/demo/action/sampledate/sampledate.ftl</result>
		</action>
		
		<action name="samplefile_*" class="panda.demo.action.samplefile.SampleFileExAction" method="{1}">
			<result name="success" type="freemarker">/panda/demo/action/samplefile/samplefile.ftl</result>
		</action>
		
		<action name="samplejoin_*" class="panda.demo.action.samplejoin.SampleJoinAction" method="{1}">
			<result name="success" type="freemarker">/panda/demo/action/samplejoin/samplejoin.ftl</result>
		</action>
		
		<action name="sampletags_*" class="panda.demo.action.sampletags.SampleTagsAction" method="{1}">
			<result name="success" type="freemarker">/panda/demo/action/sampletags/sampletags.ftl</result>
		</action>
		
		<action name="sampleunique_*" class="panda.demo.action.sampleunique.SampleUniqueAction" method="{1}">
			<result name="success" type="freemarker">/panda/demo/action/sampleunique/sampleunique.ftl</result>
		</action>
		
		<action name="cronjobs" class="panda.aems.action.tools.CronJobsAction">
			<result name="input" type="freemarker">/panda/aems/action/tools/CronJobs.ftl</result>
			<result name="success" type="freemarker">/panda/aems/action/tools/CronJobs.ftl</result>
		</action>

		<action name="dataimp" class="panda.aems.action.tools.BeanImportAction">
			<result name="input" type="freemarker">/panda/exts/struts2/actions/DataImport.ftl</result>
		</action>

		<action name="pdf" class="panda.exts.struts2.actions.Html2PdfAction">
			<result name="input" type="freemarker">/panda/exts/struts2/actions/Html2Pdf.ftl</result>
		</action>

		<action name="ndfsfile_*" class="panda.aems.action.ndfsfile.NdfsFileExAction" method="{1}">
			<result name="success" type="freemarker">/panda/aems/action/ndfsfile/ndfsfile.ftl</result>
		</action>

		<action name="ognleval" class="panda.exts.struts2.actions.OgnlEvaluateAction">
			<result name="input" type="freemarker">/panda/exts/struts2/actions/OgnlEvaluate.ftl</result>
			<result name="success" type="xml">expr, value</result>
		</action>

		<action name="sqlexec" class="panda.exts.struts2.actions.SqlExecuteAction">
			<result name="input" type="freemarker">/panda/exts/struts2/actions/SqlExecute.ftl</result>
		</action>

		<action name="error_*" class="panda.demo.action.sample.ErrorRaiseAction" method="{1}">
			<result name="input" type="freemarker">/panda/demo/action/sample/error.ftl</result>
			<result name="success" type="freemarker">/panda/demo/action/sample/error.ftl</result>
		</action>

		<action name="debug_*" class="panda.demo.action.sample.DebugAction" method="{1}">
			<result name="success" type="freemarker">/panda/exts/struts2/views/debug.ftl</result>
			<result name="json" type="json">headers, params</result>
			<result name="xml" type="xml">headers, params</result>
		</action>
	</package>
</struts>
