<!DOCTYPE validators PUBLIC
		"-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">


<validators>

	<field name="id">
		<field-validator type="conversion">
			<message key="validation-conversion-Long"/>
		</field-validator>
	</field>
	<field name="name">
		<field-validator type="stringlength">
			<param name="maxLength">100</param>
			<message key="validation-stringlength"/>
		</field-validator>
		<field-validator type="conversion">
			<message key="validation-conversion-String"/>
		</field-validator>
	</field>
	<field name="compositeUnique1">
		<field-validator type="stringlength">
			<param name="maxLength">10</param>
			<message key="validation-stringlength"/>
		</field-validator>
		<field-validator type="conversion">
			<message key="validation-conversion-String"/>
		</field-validator>
	</field>
	<field name="compositeUnique2">
		<field-validator type="stringlength">
			<param name="maxLength">10</param>
			<message key="validation-stringlength"/>
		</field-validator>
		<field-validator type="conversion">
			<message key="validation-conversion-String"/>
		</field-validator>
	</field>
</validators>
