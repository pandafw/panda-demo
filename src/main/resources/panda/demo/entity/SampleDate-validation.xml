<!DOCTYPE validators PUBLIC
		"-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">


<validators>

	<field name="id">
		<field-validator type="conversion">
			<message key="validation-conversion-Long"/>
		</field-validator>
	</field>
	<field name="popupDatetimeField">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
	<field name="popupDateField">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
	<field name="popupTimeField">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
	<field name="inlineDatetimeField">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
	<field name="inlineDateField">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
	<field name="inlineTimeField">
		<field-validator type="conversion">
			<message key="validation-conversion-Date"/>
		</field-validator>
	</field>
</validators>
