{
	// context path
	"super_path": "/super",
	"admin_path": "/admin",

	// static base
	"panda.ui.static.base": "/static",

	// default resources
	"panda.resources.default": [ 
		"panda.demo.AppEngine", 
		"panda.demo.Application", 
		"panda.app.App", 
		"panda.mvc.Resource"
	],

	// database resource / template
	//"panda.database.template": true,
	//"panda.database.resource": true,
	//"panda.database.property": true,

	// runtime settings (reloadable)
	//"panda.settings.runtime.path": "/etc/xxx.properties",

	// runtime settings reload check delay (ms) */
	//"panda.settings.runtime.delay": 60000,

	// acceptable locale array (first element is the default)
	"panda.locale.accepts": [ "en", "ja", "zh" ],

	// cache provider: ehcache, java(default)
	//"panda.cache.provider": "ehcache",
	
	// ehcache name
	//"panda.cache.name": "_default",

	// cache max-age seconds (12h)
	"panda.cache.maxage": 43200,
	
	// wkhtmltopdf exe path
	"panda.wkhtml2pdf.path": "/usr/bin/wkhtmltopdf",
	
	// lucene location
	//"panda.lucene.location": "web://WEB-INF/_lucene",
	
	// lucene analyzer
	"panda.lucene.analyzer": "org.apache.lucene.analysis.cjk.CJKAnalyzer",

	// filepool (default: use DaoFilePool to save upload files to database)
	//"panda.filepool.local.path": "/tmp/upload,"

	// multipart max upload body size (default: 2MB)
	//"panda.multipart.body.size.max": 2097152,

	// multipart max upload file size (default: 2MB)
	//"panda.multipart.file.size.max": 2097152,

	// auth
	"panda.auth.unlogin.url": "${base}/login/?redirect=${top}",
	"panda.auth.unsecure.url": "${base}/login/?redirect=${top}",

	// crypto secret key string
	//"panda.crypto.key.secret": "== Panda Java ==",

	// crypt algorithm
	//"panda.crypto.algorithm": "AES",

	// secure user age (s)
	"panda.auth.secure.session.age": 1800,
	
	// auth user type
	"panda.auth.user.type": "panda.demo.entity.User",

	// auth ticket parameter name
	//"panda.auth.ticket.param.name": "_ticket_",

	// auth ticket cookie name
	//"panda.auth.ticket.cookie.name": "WW_TICKET",

	// auth ticket cookie age (60 * 60 * 24 * 7day)
	//"panda.auth.ticket.cookie.age": 604800,

	// executor settings
	"panda.executor.enable": true,
	//"panda.executor.name": "executor",
	//"panda.executor.pool.size.core": 1,
	//"panda.executor.pool.size.max": 1,

	// scheduler settings
	"panda.scheduler.enable": true,
	//"panda.scheduler.name": "scheduler",
	//"panda.scheduler.pool.size": 1,

	// action task scheme
	"panda.task.action.scheme": "http://localhost:8080",

	// custom load task keys
	// "panda.task.load.keys": [],

	// cron
	"panda.scheduler.crons": [{
		"url": "/task/filecleanup/start",
		"description": "Execute every 30 minutes to remove the upload temporary files",
		"token": true,
		"initialDelay": 1800000,
		"fixedDelay": 1800000
	}, {
		"url": "/task/reset/start",
		"description": "Execute every 8 hours to reset the database and index",
		"token": true,
		"cron": "0 0 */8 * * *"
	}],

	// sitemesh
	"panda.sitemesh" : {
		"excludes": [],

		"decorators": [{
				"head": "x_sitemesh=false"
			}, {
				"para": "__mesh=false"
			}, {
				"para": "__inner=true",
				"page": "/sitemesh/inner.ftl"
			}, {
				"para": "__pdf=true",
				"page": "/sitemesh/pdf.ftl"
			}, {
				"para": "__print=true",
				"page": "/sitemesh/print.ftl"
			}, {
				"para": "__popup=true",
				"page": "/sitemesh/popup.ftl"
			}, {
				"path": "/.*[/_\\.]pdf",
				"page": "/sitemesh/pdf.ftl"
			}, {
				"path": "/.*[/_\\.]print",
				"page": "/sitemesh/print.ftl"
			}, {
				"path": "/.*[/_\\.]popup",
				"page": "/sitemesh/popup.ftl"
			}, {
				"path": "/.*",
				"page": "/sitemesh/main.ftl"
			}
		]
	},

